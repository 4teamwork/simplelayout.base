<?xml version="1.0"?>
<object name="portal_actions" meta_type="Plone Actions Tool"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">

 <object name="object_simplelayout_designs" meta_type="CMF Action Category">
  <property name="title"></property>
  <object name="normal" meta_type="CMF Action"
     i18n:domain="simplelayout">
   <property name="title" i18n:translate="">Normal</property>
   <property name="description" i18n:translate=""></property>
   <property
      name="url_expr">string:./change_design/setDesignInterface?name=normal</property>
   <property name="icon_expr"></property>
   <property name="available_expr"></property>
   <property name="permissions">
    <element value="Modify portal content"/>
   </property>
   <property name="visible">True</property>
  </object>
  <object name="two-columns" meta_type="CMF Action"
          i18n:domain="simplelayout">
   <property name="title" i18n:translate="">Two columns</property>
   <property name="description"></property>
   <property
      name="url_expr">string:./change_design/setDesignInterface?name=two-columns</property>
   <property name="icon_expr"></property>
   <property name="available_expr"></property>
   <property name="permissions">
    <element value="Modify portal content"/>
   </property>
   <property name="visible">True</property>
  </object>
  <object name="two-columns-one-on-top" meta_type="CMF Action"
          i18n:domain="simplelayout">
   <property name="title" i18n:translate="">Two columns one on top</property>
   <property name="description"></property>
   <property
      name="url_expr">string:./change_design/setDesignInterface?name=two-columns-one-on-top</property>
   <property name="icon_expr"></property>
   <property name="available_expr"></property>
   <property name="permissions">
    <element value="Modify portal content"/>
   </property>
   <property name="visible">True</property>
  </object>
 </object>
</object>
